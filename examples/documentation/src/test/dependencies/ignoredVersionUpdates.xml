<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">
            .*[-_\.](alpha|Alpha|ALPHA|beta|Beta|BETA|rc|RC|M|EA)[-_\.]?[0-9]?.*
        </ignoreVersion>
    </ignoreVersions>
    <rules>
        <rule groupId="org.apache.maven.plugins" comparisonMethod="maven-surefire-plugin">
            <ignoreVersions>
                <ignoreVersion>3.0.0-M5</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.eclipse.jetty" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">11\..*\..*</ignoreVersion>
                <ignoreVersion type="regex">10\..*\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.jetbrains.kotlin" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">1\.5\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
