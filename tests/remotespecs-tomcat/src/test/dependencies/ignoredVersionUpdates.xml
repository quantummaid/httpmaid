<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">
            .*[-_\.](alpha|Alpha|ALPHA|beta|Beta|BETA|rc|RC|M|EA)[-_\.]?[0-9]?.*
        </ignoreVersion>
    </ignoreVersions>
    <rules>
        <rule groupId="org.apache.maven.plugins" comparisonMethod="maven-surefire-plugin">
            <ignoreVersions>
                <ignoreVersion>3.0.0-M5</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.apache.tomcat" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">10\..*</ignoreVersion>
                <ignoreVersion type="regex">9\..*</ignoreVersion>
                <ignoreVersion>8.5.66</ignoreVersion>
                <ignoreVersion>8.5.67</ignoreVersion>
                <ignoreVersion>8.5.68</ignoreVersion>
                <ignoreVersion>8.5.69</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
